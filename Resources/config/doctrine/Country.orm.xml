<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Miky\Bundle\LocationBundle\Repository\CountryRepository" name="Miky\Bundle\LocationBundle\Entity\Country">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" column="name" nullable="true"/>
    <field name="shortName" type="string" column="shortName" nullable="true"/>
    <field name="continent" type="string" nullable="true"/>
    <field name="capital" type="string" nullable="true"/>
    <field name="phoneCode" type="string" nullable="true"/>
    <field name="flag" type="string" nullable="true"/>
    <field name="currency" type="string" nullable="true"/>
  </entity>
</doctrine-mapping>
